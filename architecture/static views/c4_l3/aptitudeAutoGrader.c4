model {
    extend aptitudeAutoGrader {
        container multipleChoice {
            description "Multiple Choice"
        }
        container gradeDatabase {
            description "Aptitude Test Grade Database"
            style {
                shape cylinder
            }
        }
    }

    aptitudeAutoGrader.multipleChoice -> aptitudeAutoGrader.gradeDatabase "saves graded multiple choice answers"
}